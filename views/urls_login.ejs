<!DOCTYPE html>
<html>
    <body>
        <h1>Login Page</h1>
        <form class="login" action="/login" method="POST">
            <label for="email">Email:</label>
            <input id="email" type="text" name="email" style="width: 300px" />
            <label for="password">Password:</label>
            <input
                id="password"
                type="password"
                name="password"
                style="width: 300px"
            />
            <input class="login" type="submit" value="Login" />
        </form>
    </body>
</html>
